{
  "name": "finx-language",
  "displayName": "finx-language",
  "description": "Finx Programming Language Support.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "finx",
      "aliases": ["Finx", "finx"],
      "extensions": [".finx", ".finxo", ".finxp", ".finxc"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icon.png",
        "dark": "./icon.png"
      }
    }],
    "grammars": [{
      "language": "finx",
      "scopeName": "source.finx",
      "path": "./syntaxes/finx.tmLanguage.json"
    }]
  }
}
